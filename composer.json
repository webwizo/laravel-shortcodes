{
    "name": "webwizo/laravel-shortcodes",
    "type": "library",
    "description": "Wordpress like shortcodes for Laravel 5.x",
    "keywords": [
        "laravel",
        "wordpress",
        "shortcodes"
    ],
    "homepage": "https://github.com/webwizo/laravel-shortcodes",
    "license": "MIT",
    "authors": [
        {
            "name": "Asif Iqbal",
            "email": "webwizo@gmail.com",
            "homepage": "http://www.webwizo.com",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/view": "~5.5.0",
        "illuminate/support": "~5.5.0",
        "illuminate/contracts": "~5.5.0",
        "php": "~7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "6.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3",
        "orchestra/testbench": "~3.5"
    },
    "autoload": {
        "classmap": [
            "tests/TestCase.php"
        ],
        "psr-4": {
            "Webwizo\\Shortcodes\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Webwizo\\Shortcodes\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
