{
    "name": "webwizo/laravel-shortcodes",
    "type": "library",
    "description": "Wordpress like shortcodes for Laravel 5, 6, and 7",
    "keywords": [
        "laravel",
        "wordpress",
        "shortcodes"
    ],
    "homepage": "https://github.com/webwizo/laravel-shortcodes",
    "license": "MIT",
    "authors": [
        {
            "name": "Asif Iqbal",
            "email": "webwizo@gmail.com",
            "homepage": "http://www.webwizo.com",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/view": "5.6.x|5.7.x|5.8.x|^6.0|^7.0",
        "illuminate/support": "5.6.x|5.7.x|5.8.x|^6.0|^7.0",
        "illuminate/contracts": "5.6.x|5.7.x|5.8.x|^6.0|^7.0",
        "php": "^7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5|^8.0",
        "orchestra/testbench": "~3.8.0",
        "scrutinizer/ocular": "^1.5",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "classmap": [
            "tests/TestCase.php"
        ],
        "psr-4": {
            "Webwizo\\Shortcodes\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Webwizo\\Shortcodes\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Webwizo\\Shortcodes\\ShortcodesServiceProvider"
            ],
            "aliases": {
                "Shortcode": "Webwizo\\Shortcodes\\Facades\\Shortcode"
            }
        }
    }
}
